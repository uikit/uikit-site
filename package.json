{
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "postinstall": "symlink-dir ./node_modules/uikit ./static/assets/uikit && cd ./static/assets/uikit && pnpm install",
        "compile": "pnpm compile-less && pnpm compile-uikit-tests",
        "compile-less": "make-dir static/css && lessc less/theme.less > static/css/theme.css --rewrite-urls=all --clean-css",
        "compile-uikit-tests": "cd ./static/assets/uikit && pnpm compile-js tests",
        "eslint": "eslint .",
        "eslint-fix": "eslint --fix ."
    },
    "packageManager": "pnpm@8.6.3",
    "devDependencies": {
        "@docsearch/css": "^3.5.1",
        "@docsearch/js": "^3.5.1",
        "@sveltejs/adapter-static": "2.0.2",
        "@sveltejs/kit": "^1.22.0",
        "clipboard": "2.0.11",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-svelte": "^2.32.2",
        "execa": "^7.1.1",
        "highlight.js": "9.12.0",
        "less": "^4.1.3",
        "less-plugin-clean-css": "^1.5.1",
        "make-dir-cli": "^3.1.0",
        "marked": "^5.1.2",
        "prettier": "^3.0.0",
        "prettier-plugin-organize-imports": "^3.2.2",
        "prettier-plugin-svelte": "^2.10.1",
        "svelte": "^4.0.4",
        "symlink-dir": "^5.1.1",
        "uikit": "3.16.22",
        "vite": "^4.4.0"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@algolia/client-search"
            ]
        }
    },
    "type": "module"
}
