{
    "root": true,
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["svelte3"],
    "overrides": [{ "files": ["*.svelte"], "processor": "svelte3/svelte3" }],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2022
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "UIkit": true,
        "ClipboardJS": true
    }
}
